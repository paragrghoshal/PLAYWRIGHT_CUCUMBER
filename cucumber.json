{
    "default": {
        "formatOptons": {
            "snippetInterface": "async-await"
        },
        "paths": [
            "tests/features/"
        ],
        "publishQuiet": true,
        "dryRun": false,
        "require": [
            "src/steps/*.ts",
            "src/hooks/*.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/reports/html/cucumber-report.html",
            "json:test-results/reports/json/cucumber-report.json",
            "rerun:@rerun.txt"
        ],
        "parallel": 0
    },
    "rerun_profile": {
        "formatOptons": {
            "snippetInterface": "async-await"
        },
        "publishQuiet": true,
        "dryRun": false,
        "require": [
            "src/steps/*.ts",
            "src/hooks/*.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/reports/html/cucumber-report.html",
            "json:test-results/reports/json/cucumber-report.json",
            "@saucelabs/cucumber-reporter",
            "rerun:@rerun.txt"
        ],
        "parallel": 2
    }
}